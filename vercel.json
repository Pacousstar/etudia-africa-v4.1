{
  "version": 2,
  "name": "etudia-africa-v4-1",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://etudia-africa-v4-1.onrender.com/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/landing.html",
      "permanent": false
    },
    {
      "source": "/accueil",
      "destination": "/landing.html", 
      "permanent": false
    },
    {
      "source": "/admin",
      "destination": "/frontend/",
      "permanent": false
    },
    {
      "source": "/react",
      "destination": "/frontend/",
      "permanent": false
    },
    {
      "source": "/interface",
      "destination": "/frontend/",
      "permanent": false
    }
  ]
}
