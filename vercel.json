{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/home",
      "dest": "/landing.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    }
  ]
}
