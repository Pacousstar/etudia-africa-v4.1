{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/home",
      "dest": "/landing.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/backoffices/eleves",
      "dest": "/backoffices/eleves/index.html"
    },
    {
      "src": "/backoffices/enseignants",
      "dest": "/backoffices/enseignants/index.html"
    },
    {
      "src": "/backoffices/parents",
      "dest": "/backoffices/parents/index.html"
    },
    {
      "src": "/backoffices/etablissements",
      "dest": "/backoffices/etablissements/index.html"
    },
    {
      "src": "/backoffices/dren",
      "dest": "/backoffices/dren/index.html"
    },
    {
      "src": "/backoffices/partenaires",
      "dest": "/backoffices/partenaires/index.html"
    },
    {
      "src": "/backoffices/administrateurs",
      "dest": "/backoffices/administrateurs/index.html"
    },
    {
      "src": "/backoffices/analytics",
      "dest": "/backoffices/analytics/index.html"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    }
  ]
}
