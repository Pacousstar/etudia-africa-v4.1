{
  "version": 2,
  "name": "etudia-africa-v4-1",
  "redirects": [
    {
      "source": "/",
      "destination": "/landing.html",
      "permanent": false
    },
    {
      "source": "/accueil",
      "destination": "/landing.html",
      "permanent": false
    },
    {
      "source": "/admin",
      "destination": "/frontend/",
      "permanent": false
    },
    {
      "source": "/interface",
      "destination": "/frontend/",
      "permanent": false
    },
    {
      "source": "/react",
      "destination": "/frontend/",
      "permanent": false
    },
    {
      "source": "/ia",
      "destination": "/frontend/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://etudia-africa-v4-1.onrender.com/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}
